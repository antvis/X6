{
  "name": "@antv/x6-sites",
  "version": "0.4.8",
  "description": "x6 sites deployed on gh-pages",
  "scripts": {
    "start": "npm run site:develop",
    "site:develop": "gatsby develop --open -H 0.0.0.0",
    "site:build": "npm run site:clean && gatsby build --prefix-paths",
    "site:clean": "gatsby clean",
    "site:deploy": "npm run site:build && gh-pages -d public"
  },
  "keywords": [
    "x6",
    "antv",
    "sites"
  ],
  "dependencies": {
    "@antv/gatsby-theme-antv": "^0.10.64",
    "@antv/x6": "^0.4.8",
    "gatsby": "^2.23.3",
    "gh-pages": "^3.0.0",
    "react-i18next": "^11.5.0"
  },
  "devDependencies": {},
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  },
  "author": {
    "name": "bubkoo",
    "email": "bubkoo.wy@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/antvis/x6",
  "bugs": {
    "url": "https://github.com/antvis/x6/issues"
  },
  "repository": "https://github.com/antvis/x6/tree/master/packages/x6-sites",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}

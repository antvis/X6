{
  "name": "@antv/x6-react-components",
  "version": "2.0.6-beta.16",
  "description": "React components for building x6 editors",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "scripts": {
    "clean": "rimraf dist es lib",
    "build:esm": "tsc --module esnext --target es2015 --outDir ./es",
    "build:cjs": "tsc --module commonjs --target es2015 --outDir ./lib",
    "build:umd": "rollup -c",
    "build:dev": "run-p build:cjs build:esm",
    "build:watch": "yarn build:esm --w",
    "build:watch:esm": "yarn build:esm --w",
    "build:watch:cjs": "yarn build:cjs --w",
    "build:ts": "run-p build:cjs build:esm build:umd",
    "build:less": "node ./scripts/less.js",
    "build": "run-s build:ts build:less",
    "prebuild": "run-s clean",
    "prepare": "run-s build:dev"
  },
  "keywords": [
    "component",
    "components",
    "react-components",
    "x6",
    "x6-editor",
    "editor",
    "ui",
    "antv"
  ],
  "peerDependencies": {
    "antd": ">=4.4.2",
    "react": ">=16.8.6 || >=17.0.0",
    "react-dom": ">=16.8.6 || >=17.0.0"
  },
  "dependencies": {
    "clamp": "^1.0.1",
    "classnames": "^2.2.6",
    "rc-dropdown": "^3.0.0-alpha.0",
    "rc-util": "^4.15.7",
    "react-color": "2.17.1",
    "react-resize-detector": "^6.6.4",
    "ua-parser-js": "^0.7.20"
  },
  "devDependencies": {
    "@types/react": "^16.9.13",
    "@types/react-color": "^3.0.1",
    "@types/react-resize-detector": "^5.0.0",
    "@types/ua-parser-js": "^0.7.33",
    "antd": "^4.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "author": {
    "name": "bubkoo",
    "email": "bubkoo.wy@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/antvis/x6",
  "bugs": {
    "url": "https://github.com/antvis/x6/issues"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/antvis/x6.git",
    "directory": "packages/x6-react-components"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "gitHead": "576fa342fa65a6867ead29f6801a30dcb31bcdb5"
}

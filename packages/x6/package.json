{
  "name": "@antv/x6",
  "version": "2.0.6-beta.26",
  "description": "JavaScript diagramming library that uses SVG and HTML for rendering.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist",
    "es",
    "lib",
    "src"
  ],
  "keywords": [
    "graph",
    "diagram",
    "flowchart",
    "uml",
    "x6-editor",
    "editor",
    "svg",
    "x6",
    "antv"
  ],
  "scripts": {
    "clean:build": "rimraf dist es lib",
    "clean:coverage": "rimraf ./test/coverage",
    "clean": "run-p clean:build clean:coverage",
    "build:esm": "tsc --module esnext --target es2015 --outDir ./es",
    "build:cjs": "tsc --module commonjs --target es2015 --outDir ./lib",
    "build:umd": "rollup -c",
    "build:less": "node ./scripts/style",
    "build:version": "node ../../scripts/version.js",
    "build:dev": "run-p build:less build:cjs build:esm",
    "build:watch": "run-s 'build:esm --w'",
    "build:watch:esm": "run-s 'build:esm --w'",
    "build:watch:cjs": "run-s 'build:cjs --w'",
    "build": "run-p build:dev build:umd",
    "prebuild": "run-s clean",
    "test": "karma start",
    "coveralls": "cat ./test/coverage/lcov.info | coveralls",
    "pretest": "run-p clean:coverage"
  },
  "dependencies": {
    "@antv/x6-common": "^2.0.6-beta.18",
    "@antv/x6-geometry": "^2.0.6-beta.18",
    "utility-types": "^3.10.0"
  },
  "author": {
    "name": "bubkoo",
    "email": "bubkoo.wy@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/antvis/x6",
  "bugs": {
    "url": "https://github.com/antvis/x6/issues"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/antvis/x6.git",
    "directory": "packages/x6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}

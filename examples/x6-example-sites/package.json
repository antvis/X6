{
  "private": true,
  "name": "@antv/x6-example-sites",
  "version": "0.10.62",
  "workspaces": [
    "packages/**/*"
  ],
  "scripts": {
    "build": "yarn workspaces run build",
    "static": "yarn speedup && yarn workspaces run deploy",
    "deploy1": "yarn speedup && wsrun -y=3 -c deploy",
    "deploy": "yarn speedup && ultra -r --concurrency '3' --silent deploy",
    "clear": "rimraf node_modules yarn.lock && yarn workspaces run clean"
  },
  "dependencies": {
    "@antv/x6": "^0.10.62",
    "@antv/x6-components": "^0.10.5",
    "@antv/x6-example-sites-helper": "^0.10.62",
    "@antv/x6-react-shape": "^0.10.7",
    "antd": "^4.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "rimraf": "^3.0.2",
    "ultra-runner": "^3.5.0",
    "wsrun": "^5.2.1"
  }
}

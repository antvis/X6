{
  "private": true,
  "name": "@antv/x6-example-sites-helper",
  "version": "0.10.42",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "config": "./scripts/config.js",
    "static": "./scripts/static.js",
    "rename": "./scripts/rename.js"
  },
  "scripts": {
    "clean": "rimraf es lib",
    "build:esm": "tsc --module esnext --target es2015 --outDir ./es",
    "build:cjs": "tsc --module commonjs --target es5 --outDir ./lib",
    "build:less": "node ./scripts/style",
    "build:watch": "yarn build:esm --w",
    "build": "run-p build:cjs build:esm build:less",
    "prebuild": "yarn clean"
  },
  "devDependencies": {
    "fs-extra": "^9.0.1",
    "less": "^3.10.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@stackblitz/sdk": "^1.4.0",
    "antd": "^4.4.2"
  },
  "peerDependencies": {
    "antd": ">=4.4.2",
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  },
  "license": "MIT"
}

{
  "private": true,
  "name": "@antv/x6-app-er",
  "version": "1.2.9",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@antv/x6": "^1.0.7",
    "@antv/x6-react-components": "^1.0.0",
    "@antv/x6-react-shape": "^1.0.7",
    "antd": "^4.4.2",
    "classnames": "^2.2.6",
    "react": "^16.12.0",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.19",
    "lint-staged": "^10.0.7",
    "prettier": "^2.1.2",
    "umi": "^3.2.19",
    "yorkie": "^2.0.0"
  }
}

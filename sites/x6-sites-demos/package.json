{
  "private": true,
  "name": "@antv/x6-sites-demos",
  "version": "1.2.1",
  "workspaces": [
    "packages/**/*"
  ],
  "scripts": {
    "build": "yarn workspaces run build",
    "deploy": "yarn speedup && yarn workspaces run deploy",
    "deploy1": "yarn speedup && wsrun -y=3 -c deploy",
    "deploy2": "yarn speedup && ultra -r --concurrency '3' --silent deploy",
    "clean": "rimraf **/node_modules"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "@antv/layout": "^0.1.9",
    "@antv/x6": "workspace:*",
    "@antv/x6-react-components": "workspace:*",
    "@antv/x6-react-shape": "workspace:*",
    "@antv/x6-sites-demos-helper": "workspace:*",
    "@types/d3-sankey": "^0.11.1",
    "@types/highlight.js": "^9.12.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "antd": "^4.4.2",
    "d3-sankey": "^0.12.3",
    "esbuild-webpack-plugin": "^1.1.0",
    "highlight.js": "^10.1.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "^3.4.1",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "ultra-runner": "^3.5.0",
    "wsrun": "^5.2.1"
  }
}

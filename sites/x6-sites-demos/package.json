    "@antv/x6-react-components": "^1.0.1",
    "@antv/x6-react-shape": "^1.1.21",
    "@antv/x6-sites-demos-helper": "^0.10.81",
    "antd": "^4.4.2",
    "esbuild-webpack-plugin": "^1.0.5",
    "highlight.js": "^10.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "rimraf": "^3.0.2",
    "ultra-runner": "^3.5.0",
    "wsrun": "^5.2.1"
  }
}
